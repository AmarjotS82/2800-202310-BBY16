
<%- include("templates/header") %>
<link rel="stylesheet" href="/css/savedRecipe.css">
<h1>Your saved recipes </h1>

<!-- This gets EJS from server side then displays results in a template made which can be seen in views/templates -->
<ul id="recipe-list">
<% let i=0 %>
<% for(i = 0; i < result.length; i++) { %>
<%- include('templates/recipe-template', {recipe: result[i]}) %>
<% } %>
</ul>
<script src="/js/recipe.js"></script>
<%- include("templates/navbar") %>
<%- include("templates/footer") %>  
